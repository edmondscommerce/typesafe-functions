{
  "name": "edmondscommerce/typesafe-functions",
  "license": "MIT",
  "type": "project",
  "description": "Type safe wrappers around internal functions that return mixed types",
  "require": {
    "php": "^7.2|^8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "edmondscommerce/phpqa": "dev-php8@dev",
    "mockery/mockery": "^1.2",
    "phpstan/phpstan-mockery": "^0.12.14",
    "phpmd/phpmd": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "ts\\": [
        "src"
      ]
    },
    "files": [
      "src/functions.php",
      "src/functions_array.php",
      "src/functions_files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ts\\Tests\\": [
        "tests"
      ]
    }
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "edmondscommerce/*": "source",
      "*": "dist"
    }
  }
}
