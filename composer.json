{
  "name": "edmondscommerce/typesafe-functions",
  "license": "MIT",
  "type": "project",
  "description": "Type safe wrappers around internal functions that return mixed types",
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpstan/phpstan-shim": "dev-master@dev",
    "edmondscommerce/phpqa": "^1.0|^2.0"
  },
  "autoload": {
    "psr-4": {
      "ts\\": [
        "src"
      ]
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ts\\Tests\\": [
        "tests"
      ]
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
